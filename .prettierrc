{
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleQuote": true,
  "singleAttributePerLine": true,
  "trailingComma": "all",
  "importOrder": [
    "^react",
    "^@react",
    "<THIRD_PARTY_MODULES>",
    "^[~/]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": false
}
